//        if (!(o instanceof OrderItems)) return false;
//        OrderItems that = (OrderItems) o;
//        return Objects.equals(id, that.id);
//    }
//
//    @Override
//    public int hashCode() {
//        return Objects.hash(id);
//    }
}
